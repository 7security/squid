msgstr "specifies the shared RADIUS secret."

#. type: TP
#: helpers/basic_auth/squid_radius_auth/squid_radius_auth.8:57
#, no-wrap
msgid "B<identifier >I<nameofserver>"
msgstr "B<identifier >I<nameofserver>"

#. type: Plain text
